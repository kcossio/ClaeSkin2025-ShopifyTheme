                      compareElement.style.display = "none";
                      document.querySelector(".g-pdp-discount").style.display = "none";
                  });
                else{
                  document.querySelector(".g-pdp-discount").style.display = "flex";
                  let currencySymbol = sale.trim().charAt(0);
                  
                  let salePrice = parseFloat(sale.replace(currencySymbol, ""));
                  let comparePrice = parseFloat(compare.replace(currencySymbol, ""));
                  let savingsDisplay = document.querySelector(".g-pdp-discount");
              
                  // Check if values are valid numbers
                  if (!isNaN(salePrice) && !isNaN(comparePrice)) {
                      let savings = comparePrice - salePrice;
                      savingsDisplay.textContent = `Save ${currencySymbol}${savings.toFixed(2)}`;
                      savingsDisplay.display = "flex";
                  } else {
                      savingsDisplay.display = "none";
                  }
                  
                  document.querySelectorAll(".g-pdp-compare").forEach(compareElement => {
                      compareElement.style.display = "block"
