                if(method === "onetime"){
                  sale = this.getAttribute("data-sale-ot");
                  compare = this.getAttribute("data-compare-ot");
                }

                document.querySelectorAll(".g-pdp-sale").forEach(saleElement => {
                  saleElement.innerHTML = sale;
                });

                document.querySelectorAll(".g-pdp-compare").forEach(compareElement => {
                  compareElement.innerHTML = compare;
                });
            
                if(sale === compare)
