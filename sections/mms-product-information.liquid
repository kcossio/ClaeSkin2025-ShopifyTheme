        const bundles = document.querySelectorAll(".g-pdp-bundle");
        bundles.forEach(bundle => {
            bundle.addEventListener("click", function(){
                bundles.forEach(b => b.classList.remove("g-pdp-bundle--selected"));

                this.classList.add("g-pdp-bundle--selected");

                const sp = this.getAttribute("data-sp");
                const vid = this.getAttribute("data-vid");
                let sale = this.getAttribute("data-sale");
                let compare = this.getAttribute("data-compare-ot");
                let save = "";
            
                const per_bottle = this.getAttribute("data-per-bottle");
                const per_serving = this.getAttribute("data-per-serving");
                const supply = this.querySelector(".g-pdp-bundle--supply").innerHTML;

                document.querySelector(".g-pdp-method-sub").setAttribute("data-sp", sp);
                document.querySelector("#selling_plan").value = sp;
                //document.querySelector("input.product-variant-id").value = vid;
                document.querySelector("#price_per_bottle").innerHTML = `${per_bottle} per bottle`;
                document.querySelector("#per_serving").innerHTML = `${per_serving} Per Serving`;
                document.querySelector("#supply_days").innerHTML = supply;


