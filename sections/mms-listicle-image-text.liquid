<style>
    .{{ section.id }} .g-image-text-wrap{
        display: flex;
    }

    .{{ section.id }} .g-image-text-wrap > div{
        width: 50%;
    }

    .{{ section.id }} .g-it-image img{
        object-fit: cover;
        width: 100%;
        height: 100%;
    }

    .{{ section.id }} .g-it-content{
        display: flex;
        align-items: center;
        background: #F8F8F8;
        text-align: center;
    }

    .{{ section.id }} .g-it-content > div{
        padding: 38px;
    }

    .{{ section.id }} .g-it-content h2{
        font-weight: 700;
        font-size: 56px;
        line-height: 58px;
        margin: 0 0 20px 0;
        text-align: center;
    }

    .{{ section.id }} .g-it-content h2 strong{
        color: #90B499;
    }

    .{{ section.id }} .g-it-content > div > p,
    .{{ section.id }} .g-it-content ul li{
        font-size: 24px;
        line-height: 24px;
        text-align: center;
    }

    .{{ section.id }} .g-it-content ul{
        margin: 0;
        padding: 0 0 0 20px;
    }

    .{{ section.id }} .g-it-content .g-cta a{
        border-radius: 30px;
        padding: 17px 0;
    }

    .{{ section.id }} .g-it-ratings{
        display: inline-flex;
        align-items: center;
        justify-content: center;
        gap: 10px;
        padding: 10px;
        background: #fff;
        margin-bottom: 10px;
    }

    .{{ section.id }} .g-it-ratings > span{
        display: flex;
    }

    .{{ section.id }} .g-it-ratings > p{
        font-size: 18px;
        font-weight: 400;
        line-height: 24px;
    }

    .{{ section.id }} .g-it-ratings p{
        margin: 0;
    }

    @media only screen and (max-width: 1024px) {
        .{{ section.id }} .g-image-text-wrap{
            flex-direction: column-reverse;
        }

        .{{ section.id }} .g-image-text-wrap > div{
            width: 100%;
        }

        .{{ section.id }} .g-it-content p, .{{ section.id }} .g-it-content ul li{
            font-size: 12px;
            line-height: 14px;
        }

        .{{ section.id }} .g-it-content > div{
            padding: 15px;
        }

        .{{ section.id }} .g-it-content h2{
            font-size: 26px;
            line-height: 31px;
            margin-bottom: 11px;
        }

        .{{ section.id }} .g-it-content ul li{
            padding-bottom: 11px;
        }

        .{{ section.id }} .g-image-text-wrap .g-cta{
            padding: 0;
            margin: 20px auto 0 auto;
        }

        .{{ section.id }} .g-image-text-wrap .g-cta a{
            max-width: unset;
            text-align: center;
            width: 100%;
        }
    }
</style>
<div class="{{ section.id }} g-image-text">
    <div class="g-image-text-wrap">
        <div class="g-it-content">
            <div>
                {% if section.settings.ratings != blank %}
                <div class="g-it-ratings">
                    <span>
